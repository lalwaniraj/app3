pipeline {
    agent any

    stages {
        stage('Checkout Code') {
            steps {
                git branch: 'main',
                    url: 'https://github.com/lalwaniraj/app3.git'
            }
        }

        stage('Build Docker Image') {
            steps {
                sh 'docker build -t app3:raj .'
            }
        }
    }
}
